<h1>Edit Public Wiki</h1>

<div class="row">
  <div class="col-md-8">
    <%= form_for @page do |f| %>
       <%= f.label :title %>
       <%= f.text_field :title, class: 'form-control', placeholder: "Enter wiki title"  %>

       <%= f.label :body %>
       <%= f.text_area :body,rows: 7, class: 'form-control', placeholder: "Enter wiki body" %>

       <%= f.submit "Save", class: 'btn btn-success' %>
     <% end %>
  </div>
+</div>
